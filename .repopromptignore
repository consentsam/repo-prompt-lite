# Ignore node modules (always ignored by default)
node_modules

# Ignore git files (always ignored by default)
.git

# Ignore build outputs
dist
build
out

# Ignore logs
*.log

# Ignore caches
.cache
.npm

# Ignore coverage reports
coverage

# Ignore environment files
.env
.env.*

# Ignore IDE and editor folders
.vscode
.idea
.DS_Store
